<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/contenedor"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:background="#ffffff"
    tools:context=".fragments.InformeFragment"
    >


    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"


        >

    <LinearLayout
        android:id="@+id/linearLayoutPadre"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        >


        <!--CONTENEDOR FECHA Y HORA-->
        <!--***********************************************************************-->
        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:gravity="center_vertical"
            android:layout_gravity="center_horizontal"
            android:layout_marginTop="@dimen/activity_vertical_margin"
            android:layout_marginBottom="@dimen/activity_vertical_margin"
            >

            <TextView
                android:id="@+id/txtXXDia"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="@dimen/text_size_standard"
                android:text="XX"
                android:textColor="@color/ColorSecundadrio"
             />




            <TextView
                android:id="@+id/txtXXMes"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="@dimen/text_size_standard"
                android:text="/XX/"
                android:textColor="@color/ColorSecundadrio"
                />

            <TextView
                android:id="@+id/txtXXAno"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="@dimen/text_size_standard"
                android:text="XXXX"
                android:textColor="@color/ColorSecundadrio"
                />

            <TextView
                android:id="@+id/txtHora"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="2dp"
                android:layout_marginEnd="2dp"
                android:textSize="@dimen/text_size_standard"
                android:text="@string/inf_hora"
                android:textColor="@color/colorPrimary"
                />


            <TextView
                android:id="@+id/txtXXHora"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="@dimen/text_size_standard"
                android:text="XX:XX"
                android:textColor="@color/colorPrimary"
                />

            <TextView
                android:id="@+id/txtDuracion"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="2dp"
                android:layout_marginEnd="2dp"
                android:textSize="@dimen/text_size_standard"
                android:text="@string/inf_duracion"
                android:textColor="@color/colorPrimary"
                />

            <TextView
                android:id="@+id/txtXXDuracion"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"

                android:textSize="@dimen/text_size_standard"
                android:text="XX:XX"
                android:textColor="@color/ColorSecundadrio"
                />

        </LinearLayout>
        <!--FIN CONTENEDOR FECHA Y HORA-->
        <!--***********************************************************************-->

        <!--CONTENEDOR CONTROL DE MOVIMIENTO-->
        <!--***********************************************************************-->
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/activity_vertical_margin"
            android:layout_marginBottom="@dimen/activity_vertical_margin"
            android:paddingStart="@dimen/activity_horizontal_margin"
            android:paddingEnd="@dimen/activity_horizontal_margin"
            android:paddingTop="@dimen/activity_vertical_margin"
            android:paddingBottom="@dimen/activity_vertical_margin"
            android:background="@color/data_fondo"
            >


                <TextView
                    android:id="@+id/txtControlMov"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:ems="5"
                    android:textSize="@dimen/text_size_subtitle"
                    android:text="@string/inf_control_mov"
                    android:textColor="@color/ColorSecundadrio"

                    />


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:orientation="horizontal"
                >

                    <LinearLayout
                        android:id="@+id/contenedor_select_poor"
                        android:layout_width="0dp"
                        android:layout_height="@dimen/inf_size_select_result"
                        android:layout_weight="0.33"

                        android:padding="@dimen/inf_size_padding_select_result"
                        android:background="@drawable/border_seleccion_result"
                        android:layout_gravity="center_vertical"
                        >

                            <LinearLayout
                                android:id="@+id/contenedor_mov_poor"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:background="@color/data_poor"
                                android:orientation="horizontal"
                                android:textColor="@color/ColorSecundadrio"
                                >

                                <TextView

                                    android:id="@+id/txt_mov_malo"
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:gravity="center"
                                    android:text="@string/inf_poor"
                                    android:textSize="@dimen/text_size_standard"

                                    />
                            </LinearLayout>

                    </LinearLayout>


                <!--android:background="@drawable/border_seleccion_result"
                    android:paddingBottom="4dp"
                    android:paddingTop="4dp"
                    android:paddingStart="4dp"
                    android:paddingEnd="4dp"
                -->
                <LinearLayout
                    android:id="@+id/contenedor_select_normal"
                    android:layout_width="0dp"
                    android:layout_height="@dimen/inf_size_select_no_result"
                    android:layout_weight="0.33"
                    android:layout_gravity="center_vertical"
                    >


                    <LinearLayout
                        android:id="@+id/contenedor_mov_normal"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:background="@color/data_normal"
                        android:orientation="horizontal"
                        android:alpha="0.4"
                        android:textColor="@color/ColorSecundadrio"
                        >

                        <TextView
                            android:id="@+id/txt_mov_normal"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:gravity="center"
                            android:textColor="@color/ColorSecundadrio"
                            android:text="@string/inf_normal"
                            android:textSize="@dimen/text_size_standard"

                            />
                    </LinearLayout>

                </LinearLayout>

                <LinearLayout
                    android:id="@+id/contenedor_select_good"
                    android:layout_width="0dp"
                    android:layout_height="@dimen/inf_size_select_no_result"
                    android:layout_weight="0.33"
                    android:layout_gravity="center_vertical"
                    >


                    <LinearLayout
                        android:id="@+id/contenedor_mov_good"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="horizontal"
                        android:background="@color/data_good"
                        android:alpha="0.4"
                        >

                        <TextView

                            android:id="@+id/txt_mov_good"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:gravity="center"
                            android:textColor="@color/ColorSecundadrio"
                            android:text="@string/inf_bueno"
                            android:textSize="@dimen/text_size_standard"

                            />
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>
        <!--FIN CONTENEDOR CONTROL DE MOVIMIENTO-->
        <!--***********************************************************************-->

        <!--CONTENEDOR PORCENTAJES DE RANGOS-->
        <LinearLayout
            android:id="@+id/contenedorPorcentajeRangos"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@color/data_fondo"
            android:orientation="vertical"
            android:layout_marginTop="@dimen/activity_vertical_margin"
            android:layout_marginBottom="@dimen/activity_vertical_margin"
            >
            <TextView
                android:id="@+id/txtTituloPorcentajesRangos"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:textColor="@color/ColorSecundadrio"
                android:text="@string/inf_porcentaje_tiempo"
                android:textSize="@dimen/text_size_subtitle"
                android:paddingStart="@dimen/activity_horizontal_margin"
                android:paddingEnd="@dimen/activity_horizontal_margin"
                android:layout_marginTop="@dimen/activity_vertical_margin"
                android:layout_marginBottom="@dimen/activity_vertical_margin"
                />

            <!--BARRA HORIZONTAL-->
            <!--NOTA : No dar menos del 8% de ancho para que quepan los numeros-->
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"


                android:paddingStart="14dp"
                android:paddingEnd="14dp"
                >


                <LinearLayout
                    android:id="@+id/barraPorcentajeRangoGoood"
                    android:layout_width="0dp"
                    android:layout_height="30dp"
                    android:layout_weight="0.20"
                    android:background="@color/data_leyenda_porcentaje_good"
                    android:orientation="horizontal"
                    android:weightSum="1">

                    <TextView
                        android:id="@+id/txtPorcentajeRangoGood"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:gravity="center"
                        android:textColor="@color/ColorSecundadrio"
                        android:textSize="@dimen/text_size_standard"
                        android:text="20%"

                        />
                </LinearLayout>
                <LinearLayout
                    android:id="@+id/barraPorcentajeRangoFair"
                    android:layout_width="0dp"
                    android:layout_height="30dp"
                    android:layout_weight="0.08"
                    android:background="@color/data_leyenda_porcentaje_fair"
                    android:orientation="horizontal" >

                    <TextView
                        android:id="@+id/txtPorcentajeRangoFair"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:gravity="center"
                        android:textColor="@color/ColorSecundadrio"
                        android:textSize="@dimen/text_size_standard"
                        android:text="8%"
                        />

                </LinearLayout>

                <LinearLayout
                    android:id="@+id/barraPorcentajeRangoBad"
                    android:layout_width="0dp"
                    android:layout_height="30dp"
                    android:layout_weight="0.50"
                    android:background="@color/data_leyenda_porcentaje_bad"
                    android:orientation="horizontal" >

                    <TextView
                        android:id="@+id/txtPorcentajeRangoBad"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:gravity="center"
                        android:textColor="@color/ColorSecundadrio"
                        android:textSize="@dimen/text_size_standard"
                        android:text="50%"
                        />

                </LinearLayout>

                <LinearLayout
                    android:id="@+id/barraPorcentajeRangoVerBad"
                    android:layout_width="0dp"
                    android:layout_height="30dp"
                    android:layout_weight="0.22"
                    android:background="@color/data_leyenda_porcentaje_very_bad"
                    android:orientation="horizontal" >

                    <TextView

                        android:id="@+id/txtPorcentajeRangoVeryBad"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:gravity="center"
                        android:textColor="@color/ColorSecundadrio"
                        android:textSize="@dimen/text_size_standard"
                        android:text="22%"
                        />

                </LinearLayout>

            </LinearLayout>
            <!--FIN BARRA HORIZONTAL-->

            <!--CORCHETES-->
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:paddingStart="14dp"
                android:paddingEnd="14dp"
                >
                    <LinearLayout
                        android:id="@+id/corchete_inferior_porcentaje_good"
                        android:layout_width="0dp"
                        android:layout_height="@dimen/activity_vertical_margin"
                        android:layout_weight="0.28"

                        android:background="@drawable/border_inf_porcentaje_good"
                        android:orientation="horizontal"
                        >

                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/corchete_inferior_porcentaje_bad"
                        android:layout_width="0dp"
                        android:layout_height="@dimen/activity_vertical_margin"
                        android:layout_weight="0.72"

                        android:background="@drawable/border_inf_porcentaje_bad"
                        android:orientation="horizontal"
                        >
                    </LinearLayout>

            </LinearLayout>
            <!--FIN CORCHETES-->

            <!--RESULTADO DE CORCHETES-->
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                >
                <LinearLayout
                    android:id="@+id/contenedor_result_corchete_good"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.28"
                    android:layout_marginStart="@dimen/activity_horizontal_margin"
                    android:orientation="horizontal"
                    >

                    <TextView
                        android:id="@+id/txt_corchetes_porcentaje_good"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center_horizontal"
                        android:textColor="@color/data_borde_inf_porcentaje_good"
                        android:textSize="@dimen/text_size_standard"
                        android:text="28%"
                        />

                </LinearLayout>



                <LinearLayout
                    android:id="@+id/contenedor_result_corchete_bad"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.72"
                    android:layout_marginEnd="@dimen/activity_horizontal_margin"
                    android:orientation="horizontal"
                    >

                    <TextView
                        android:id="@+id/txt_corchetes_porcentaje_bad"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center_horizontal"
                        android:textColor="@color/data_borde_inf_porcentaje_bad"
                        android:textSize="@dimen/text_size_standard"
                        android:text="72%"
                        />
                </LinearLayout>

            </LinearLayout>
            <!--FIN DE RESULTADO DE CORCHETES-->


            <!--Cara_inclinación, cara_flexión y leyenda-->
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:layout_marginTop="@dimen/activity_vertical_margin"
                android:layout_marginBottom="@dimen/activity_vertical_margin"
                android:gravity="center_vertical"
                >

                <!--Cara_inclinación-->
                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.28"
                    android:gravity="center"
                    android:orientation="horizontal"
                    >


                    <ImageView
                        android:id="@+id/img_porcentaje_inclinacion"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:adjustViewBounds="true"
                        android:scaleType="centerCrop"
                        android:src="@drawable/rango_inclinacion"
                     />
                </LinearLayout>

                <!--Cara_flexion-->
                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.28"
                    android:gravity="center"
                    android:orientation="horizontal"
                    >


                    <ImageView
                        android:id="@+id/img_porcentaje_flexion"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:adjustViewBounds="true"
                        android:scaleType="centerCrop"
                        android:src="@drawable/rango_flexion"
                        />


                </LinearLayout>

                <!--Leyenda-->
                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.36"
                    android:paddingStart="4dp"
                    android:paddingEnd="4dp"
                    android:orientation="vertical"
                    >


                    <!--Titulo-->
                    <TextView
                        android:id="@+id/txtTituloLeyendaRangos"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center"
                        android:textColor="@color/ColorSecundadrio"
                        android:text="@string/inf_ranks"
                        android:textSize="@dimen/text_size_subtitle"
                        />


                    <!--Contenedor Favorable-->
                    <LinearLayout
                        android:id="@+id/Contendor_porcentaje_favorable"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:paddingBottom="@dimen/activity_vertical_margin"
                        android:paddingTop="@dimen/activity_vertical_margin"
                        android:paddingStart="5dp"
                        android:paddingEnd="5dp"
                        android:layout_gravity="center_horizontal"
                        android:background="@color/data_fondo_porcentaje_good"
                        android:orientation="vertical"
                        >

                        <!--Bueno-->
                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal"

                            ><!--BUENO-->

                                <LinearLayout
                                    android:id="@+id/contenedor_leyenda_good"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:orientation="horizontal"
                                    >


                                    <TextView
                                        android:id="@+id/leyenda_good"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:textColor="@color/ColorSecundadrio"
                                        android:text="@string/inf_bueno"
                                        android:textSize="@dimen/text_size_standard"
                                        android:layout_gravity="center_vertical"
                                        android:ems="3"

                                        />

                                        <LinearLayout
                                            android:layout_width="18dp"
                                            android:layout_height="18dp"
                                            android:background="@color/data_leyenda_porcentaje_good"
                                            android:layout_marginStart="@dimen/margin_leyenda_size_color_horizontal"
                                            android:layout_marginEnd="@dimen/margin_leyenda_size_color_horizontal"
                                            >
                                        </LinearLayout>

                                    <TextView
                                        android:id="@+id/txt_leyenda_good"
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:textColor="@color/ColorSecundadrio"
                                        android:text="20%"
                                        android:layout_gravity="center_vertical"
                                        android:textSize="@dimen/text_size_standard"
                                        />
                                </LinearLayout>

                        </LinearLayout>
                        <!--FIN BUENO-->

                        <!--Favorable-->
                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal"
                            android:layout_marginTop="@dimen/activity_vertical_margin"
                            android:layout_gravity="start"
                            ><!--FAVORABLE-->

                            <RelativeLayout
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                >

                                <LinearLayout
                                    android:id="@+id/contenedor_leyenda_fair"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:orientation="horizontal"
                                    >


                                    <TextView
                                        android:id="@+id/leyenda_fair"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:textColor="@color/ColorSecundadrio"
                                        android:text="@string/inf_favorable"
                                        android:textSize="@dimen/text_size_standard"
                                        android:ems="3"
                                        />


                                    <LinearLayout
                                        android:layout_width="18dp"
                                        android:layout_height="18dp"
                                        android:background="@color/data_leyenda_porcentaje_fair"
                                        android:layout_marginStart="@dimen/margin_leyenda_size_color_horizontal"
                                        android:layout_marginEnd="@dimen/margin_leyenda_size_color_horizontal"
                                        >


                                    </LinearLayout>

                                    <TextView
                                        android:id="@+id/txt_leyenda_fair"
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:textColor="@color/ColorSecundadrio"
                                        android:text="8%"
                                        android:textSize="@dimen/text_size_standard"
                                        />
                                </LinearLayout>
                            </RelativeLayout>

                        </LinearLayout>
                        <!--FIN FAVORABLE-->


                    </LinearLayout>
                    <!--Fin contenedor favorables-->


                    <!--contendor DESFAVORABLE-->
                    <LinearLayout
                        android:id="@+id/Contendor_porcentaje_bad"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:paddingBottom="@dimen/activity_vertical_margin"
                        android:paddingTop="@dimen/activity_vertical_margin"
                        android:paddingStart="5dp"
                        android:paddingEnd="5dp"
                        android:layout_gravity="center_horizontal"
                        android:background="@color/data_fondo_porcentaje_bad"
                        android:orientation="vertical"
                        >

                        <!--BAD-->
                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal"

                            >

                            <LinearLayout
                                android:id="@+id/contenedor_leyenda_bad"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:orientation="horizontal"
                                >


                                <TextView
                                    android:id="@+id/leyenda_bad"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:textColor="@color/ColorSecundadrio"
                                    android:text="@string/inf_malo"
                                    android:textSize="@dimen/text_size_standard"
                                    android:layout_gravity="center_vertical"
                                    android:ems="3"
                                    />

                                <LinearLayout
                                    android:layout_width="18dp"
                                    android:layout_height="18dp"
                                    android:background="@color/data_leyenda_porcentaje_bad"
                                    android:layout_marginStart="@dimen/margin_leyenda_size_color_horizontal"
                                    android:layout_marginEnd="@dimen/margin_leyenda_size_color_horizontal"
                                    >
                                </LinearLayout>


                                <TextView
                                    android:id="@+id/txt_leyenda_bad"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:textColor="@color/ColorSecundadrio"
                                    android:text="8%"
                                    android:layout_gravity="center_vertical"
                                    android:textSize="@dimen/text_size_standard"
                                    />
                            </LinearLayout>
                        </LinearLayout>
                        <!--FIN BAD-->


                        <!--VERY BAD-->
                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal"
                            android:layout_marginTop="@dimen/activity_vertical_margin"
                            android:layout_gravity="start"
                            >

                            <RelativeLayout
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                >

                                <LinearLayout
                                    android:id="@+id/contenedor_leyenda_"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:orientation="horizontal"
                                    >

                                    <TextView
                                        android:id="@+id/leyenda_ver_bad"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:textColor="@color/ColorSecundadrio"
                                        android:text="@string/inf_muy_malo"
                                        android:textSize="@dimen/text_size_standard"
                                        android:ems="3"
                                        />

                                    <LinearLayout
                                        android:layout_width="18dp"
                                        android:layout_height="18dp"
                                        android:background="@color/data_leyenda_porcentaje_very_bad"
                                        android:layout_marginStart="@dimen/margin_leyenda_size_color_horizontal"
                                        android:layout_marginEnd="@dimen/margin_leyenda_size_color_horizontal"
                                        >


                                    </LinearLayout>

                                    <TextView
                                        android:id="@+id/txt_leyenda_very_bad"
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:textColor="@color/ColorSecundadrio"
                                        android:text="22%"
                                        android:textSize="@dimen/text_size_standard"
                                        />

                                </LinearLayout>


                            </RelativeLayout>

                        </LinearLayout>
                        <!--FIN VERY BAD-->

                    </LinearLayout>
                    <!--Fin contenedor DESFAVORABLE-->


                </LinearLayout>
                <!--fIN LEYENDA-->

            </LinearLayout>
            <!--FIN Cara_inclinación, cara_flexión y leyenda-->

        </LinearLayout>
        <!--***************************************************************************-->
        <!--FIN CONTENDOR PORCENTAJE RANGOS-->


        <!--CONTENEDOR GRAFICA LINEAL-->
        <!--***************************************************************************-->
        <LinearLayout
            android:layout_marginTop="@dimen/activity_vertical_margin"
            android:layout_marginBottom="@dimen/activity_vertical_margin"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@color/data_fondo"

            android:orientation="vertical"
            >

            <ImageView

                    android:layout_gravity="center_horizontal"
                    android:id="@+id/imgLeyenda_grafico_lineal"
                    android:layout_width="wrap_content"
                    android:layout_height="@dimen/height_leyenda_lineal"
                    android:adjustViewBounds="true"
                    android:scaleType="centerCrop"
                    android:src="@drawable/leyenda_grafica_lineal_position"


             />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/height_grafica_lineal"

                >

                <com.github.mikephil.charting.charts.LineChart
                    android:id="@+id/chart1"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    />

            </LinearLayout>

        </LinearLayout>
        <!--***************************************************************************-->
        <!--FIN CONTENEDOR GRAFICA LINEAL-->



        <!--CONTENEDOR Inclinación-->
            <LinearLayout
